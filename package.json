{
  "name": "beyond-the-five",
  "description": "The website for Beyond The Five",
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "export": "next build && next export"
  },
  "version": "1.0.0",
  "main": "pages/index.js",
  "author": "Joshua Martinez",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/display-name": "off",
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ]
    },
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    }
  },
  "dependencies": {
    "@rebass/preset": "^4.0.5",
    "@zeit/next-css": "^1.0.1",
    "emotion-theming": "^10.0.27",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.1",
    "next-fonts": "^1.0.3",
    "next-images": "^1.3.1",
    "react": "^16.13.1",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-spinners": "^0.8.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.0.1",
    "styled-jsx": "^3.2.5"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0"
  }
}
