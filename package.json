{
	"name": "beyond-the-five",
	"description": "The website for Beyond The Five",
	"scripts": {
		"start": "next start",
		"build": "next build",
		"dev": "next dev",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"export": "next build && next export"
	},
	"version": "1.0.0",
	"main": "pages/index.js",
	"author": "Joshua Martinez",
	"license": "MIT",
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"rules": {
			"react/react-in-jsx-scope": "off",
			"react/display-name": "off",
			"semi": [
				"error",
				"never"
			],
			"quotes": [
				"error",
				"single"
			]
		},
		"parser": "babel-eslint",
		"env": {
			"node": true,
			"browser": true,
			"es6": true
		}
	},
	"dependencies": {
		"@rebass/preset": "^4.0.5",
		"@zeit/next-css": "^1.0.1",
		"emotion-theming": "^10.0.27",
		"next": "^9.2.1",
		"next-images": "^1.3.0",
		"react": "^16.12.0",
		"react-collapse": "^5.0.1",
		"react-dom": "^16.12.0",
		"react-icons": "^3.8.0",
		"rebass": "^4.0.7",
		"styled-components": "^5.0.0"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.18.0"
	}
}
